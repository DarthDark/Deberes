
estado = input("Introduce estado de Ã¡nimo: ")

mood = "happy"

if estado == "happy":
    print("It is great to see you happy!")

elif estado == "nervioso":
    print("Take a deep breath 3 times.")

elif estado == "triste":
    print("A llorar.")

elif estado == "emocionado":
    print("lagrimilla...")

elif estado == "relajado":
    print("Viva la vida.")

else:
    print("I don't recognize this mood")
